# Base class: Animal
class Animal:
    def __init__(self, name):
        self.name = name
    
    def move(self):
        pass  # This will be overridden by subclasses


# Subclass 1: Dog (inherits from Animal)
class Dog(Animal):
    def move(self):
        print(f"{self.name} is running on four legs ğŸ•ğŸƒâ€â™‚ï¸")


# Subclass 2: Fish (inherits from Animal)
class Fish(Animal):
    def move(self):
        print(f"{self.name} is swimming in the water ğŸŸğŸ’¦")


# Subclass 3: Bird (inherits from Animal)
class Bird(Animal):
    def move(self):
        print(f"{self.name} is flying in the sky ğŸ¦…ğŸŒ¤ï¸")


# Testing the animals
animals = [Dog("Buddy"), Fish("Nemo"), Bird("Sky")]

for animal in animals:
    animal.move()
